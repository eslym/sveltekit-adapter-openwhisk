        "mrmime": "^1.0.1",
        "prettier": "^2.8.7",
        "pretty-quick": "^3.1.3",
        "rimraf": "^5.0.0",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4",
        "vite": "^4.2.1"
    },
    "dependencies": {
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "rollup": "^3.7.0"
