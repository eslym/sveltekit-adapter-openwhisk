{
    "name": "@eslym/sveltekit-adapter-openwhisk",
    "version": "1.0.0-beta.1",
    "type": "module",
    "description": "SvelteKit adapter for OpenWhisk web action",
    "main": "dist/index.js",
    "author": "0nepeop1e",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/eslym/sveltekit-adapter-openwhisk"
    },
    "scripts": {
        "build": "rimraf dist && rollup -c",
        "format": "prettier --write ."
    },
